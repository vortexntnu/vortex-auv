<?xml version="1.0"?>
<package format="3">
  <name>landmark_server</name>
  <version>0.0.0</version>
  <description>Landmark server for mission planning (skeleton)</description>
  <maintainer email="jorgen.fjermedal@hotmail.com">Jorgen Fjermedal</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>geometry_msgs</depend>
  <depend>vortex_msgs</depend>
  <depend>vortex_utils</depend>
  <depend>pose_filtering</depend>
  <depend>vortex_utils_ros_tf</depend>
  <depend>vortex_utils_ros</depend>
  <depend>tf2_geometry_msgs</depend>

  <test_depend>launch_testing</test_depend>
  <test_depend>launch_testing_ros</test_depend>
  <test_depend>launch_testing_ament_cmake</test_depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
