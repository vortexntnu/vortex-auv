<?xml version="1.0"?>
<?xml-model href="http://download.ros.org/schema/package_format3.xsd" schematypens="http://www.w3.org/2001/XMLSchema"?>
<package format="3">
  <name>ipda_pose_filtering</name>
  <version>0.0.0</version>
  <description>IPDA pose filtering and tracking package, with a C++ track manager and ROS 2 interface node.</description>

  <maintainer email="jorgen.fjermedal@hotmail.com">jorgen</maintainer>
  <license>MIT</license>

  <buildtool_depend>ament_cmake</buildtool_depend>

  <depend>rclcpp</depend>
  <depend>rclcpp_components</depend>
  <depend>geometry_msgs</depend>
  <depend>tf2</depend>
  <depend>tf2_ros</depend>
  <depend>tf2_geometry_msgs</depend>
  <depend>message_filters</depend>
  <depend>vortex_utils</depend>
  <depend>vortex_filtering</depend>

  <export>
    <build_type>ament_cmake</build_type>
  </export>
</package>
